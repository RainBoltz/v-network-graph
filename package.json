{
  "name": "v-network-graph",
  "description": "An interactive network graph visualization component for Vue 3",
  "homepage": "https://dash14.github.io/v-network-graph/",
  "repository": {
    "type": "git",
    "url": "https://github.com/dash14/v-network-graph.git"
  },
  "version": "0.0.3",
  "license": "MIT",
  "files": [
    "lib"
  ],
  "main": "./lib/index.umd.js",
  "module": "./lib/index.es.js",
  "scripts": {
    "dev": "vite",
    "build": "run-s clean build:tc build:lib",
    "build:tc": "vue-tsc --noEmit",
    "build:lib": "run-p build:lib:*",
    "build:lib:main": "vite --config vite-lib.config.ts build",
    "build:lib:force": "vite --config vite-lib-force.config.ts build",
    "build:example": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "clean": "rimraf lib"
  },
  "dependencies": {
    "mitt": "^3.0.0",
    "svg-pan-zoom": "github:dash14/svg-pan-zoom#main"
  },
  "devDependencies": {
    "@types/d3-force": "^3.0.3",
    "@types/lodash-es": "^4.17.4",
    "@types/node": "^16.6.1",
    "@types/victor": "^1.1.0",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "@vitejs/plugin-vue": "^1.4.0",
    "@vue/compiler-sfc": "^3.2.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vue": "^7.16.0",
    "lodash-es": "^4.17.21",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "sass": "^1.37.5",
    "typescript": "^4.3.5",
    "victor": "^1.1.0",
    "vite": "^2.4.4",
    "vite-plugin-dts": "^0.6.0",
    "vue-tsc": "^0.2.2"
  },
  "peerDependencies": {
    "d3-force": "^3.0.0",
    "vue": "^3.2.2"
  },
  "exports": {
    ".": {
      "import": "./lib/index.es.js",
      "require": "./lib/index.umd.js"
    },
    "./force-layout": {
      "import": "./lib/force-layout.es.js",
      "require": "./lib/force-layout.umd.js"
    },
    "./style.css": "./lib/style.css"
  },
  "types": "./lib/types/index.d.ts",
  "typesVersions": {
    "*": {
      "lib/types/index.d.ts": [
        "lib/types/index.d.ts"
      ],
      "*": [
        "lib/types/*"
      ]
    }
  }
}
